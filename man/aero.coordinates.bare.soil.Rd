## The function is currently defined as
function (filepath, gdb) 
{
    library(arcgisbinding)
    arcgisbinding::arc.check_product()
    plots <- read.geodatabase(filepath = filepath, gdb = gdb, 
        feature.name = "SV_IND_TERRESTRIALAIM")
    coordinates.bare.soil <- plots[, c("PrimaryKey", "Latitude", 
        "Longitude", "BareSoilCover_FH")]
    return(coordinates.bare.soil)
  }
}
